    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^25.3.0",
    "parcel-bundler": "^1.12.4"
  },
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.9.2"
  }
